required:
- dataset
- table

properties:
  dataset:
    type: string
    description: Name of BigQuery Dataset to create.

  table:
    type: string
    description: Name of BigQuery Table to create.

outputs:
  ip:
    type: string
    description: The BigQuery connection string (bigquery://<project>.<dataset>.<table>).
  port:
    type: string
    description: base64 encoded JSON credentials file.
